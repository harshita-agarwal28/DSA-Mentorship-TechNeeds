#include <iostream>
using namespace std;
int main(){
    int t;
    cin >> t;
    int n;
    int i=1;
    while (i<=t){
        cin>>n;
        for(int j=2;j<n;j++){
            if (n%j==0){
                cout<<"not prime";
                break;
            }
        else{
            cout<<"prime";
        }
        }
        i+=1;
    }

    
}
